{
  "name": "px-viewport",
  "publisher": "PolDevInc",
  "displayName": "px-viewport",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:px-viewport.helloWorld",
    "onCommand:px-viewport.vh",
    "onCommand:px-viewport.vw",
    "onCommand:px-viewport.em"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "px-viewport.helloWorld",
        "title": "Hello World"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+alt+h",
        "command": "px-viewport.vh"
      },
      {
        "key": "ctrl+alt+w",
        "command": "px-viewport.vw"
      },
      {
        "key": "ctrl+alt+e",
        "command": "px-viewport.em"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.44.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}
